<h1>New gemm</h1>

<% form_for(@gemm) do |f| %>
  <%= f.error_messages %>

      <p>
      <%= f.label :name %><br />
      <%= f.text_field :name %>
    </p>
      <p>
      <%= f.label :version %><br />
      <%= f.text_field :version %>
    </p>
  
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', gemms_path %>
